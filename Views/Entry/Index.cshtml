@model IEnumerable<ekozigPersonEntryDemo.Models.Entry>

<!DOCTYPE html>
<html>
<head>
    <title>List</title>
</head>
<body>
    <h1>List</h1>
    <table border="1">
        <thead>
            <tr>
                <th>ID</th>
                <th>FirstName</th>
                <th>LastName</th>
                <th>Email</th>
                <th>Phone</th>
                <th>Sex</th>
                <th>PostCode</th>
                <th>Town</th>
                <th>Street</th>
                <th>StreetType</th>
                <th>HouseNumber</th>
                <th>Floor</th>
                <th>Door</th>
                <th>RingNumber</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var product in Model)
            {
                <tr>
                    <td>@product.Id</td>
                    <td>@product.FirstName</td>
                    <td>@product.LastName</td>
                    <td>@product.Email</td>
                    <td>@product.Phone</td>
                    <td>@product.Sex</td>
                    <td>@product.Address.PostCode</td>
                    <td>@product.Address.Town</td>
                    <td>@product.Address.Street</td>
                    <td>@product.Address.StreetType</td>
                    <td>@product.Address.HouseNumber</td>
                    <td>@product.Address.Floor</td>
                    <td>@product.Address.Door</td>
                    <td>@product.Address.RingNumber</td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>
